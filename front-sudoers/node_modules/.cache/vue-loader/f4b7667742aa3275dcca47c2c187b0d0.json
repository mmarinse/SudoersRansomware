xt>\n    </v-card>\n  </v-dialog>\n</template>\n<script>\nexport default {\n  props: {\n    dialog: {\n      type: Boolean,\n      default: false\n    }\n  },\n  data() {\n    return {\n      cookiesMessage: true,\n      lastCookies: null,\n    }\n  },\n  watch: {\n    open(value) {\n      this.dialog = value;\n    },\n    dialog(value) {\n      if (!value) {\n        this.$emit('dialog-closed', false);\n      }\n      if(window.hbspt){\n        this.cookiesMessage = false\n        window.hbspt.forms.create({\n          region: \"na1\",\n          portalId: \"4764254\",\n          formId: this.$i18n.locale === \"es\" ? \"4bef50b8-b94d-43ac-bf3a-f3eebb369a2e\" : \"e6e57e85-223e-4f07-8b97-7c9254bec58e\" ,\n          target:'#request_demo_form',\n        });\n      }\n    }\n  },\n  methods: {\n    listenCookies: function(){\n      setInterval(function(){\n        let currentCookie = document.cookie;\n        if (currentCookie != this.lastCookies) {\n          this.lastCookies = currentCookie; // store latest cookie\n          if(window.hbspt){\n            this.cookiesMessage = false\n            window.hbspt.forms.create({\n              region: \"na1\",\n              portalId: \"4764254\",\n              formId: this.$i18n.locale === \"es\" ? \"4bef50b8-b94d-43ac-bf3a-f3eebb369a2e\" : \"e6e57e85-223e-4f07-8b97-7c9254bec58e\" ,\n              target:'#request_demo_form',\n            });\n            this.loading = false\n          }\n        }\n      }.bind(this), 1000);\n    }\n  },\n  mounted(){\n    if(window.hbspt){\n      this.cookiesMessage = false\n    }\n    this.listenCookies()\n  }\n}\n</script>\n<style lang=\"scss\">\n//Form field error\n#request_demo_form .error {\n  margin: 10px 0;\n  background-color: white !important;\n  border: 2px solid indianred !important;\n  border-radius: 5px;\n}\n//.v-application .error {\n//  margin: 10px 0;\n//  background-color: white !important;\n//  border: 2px solid indianred !important;\n//  border-radius: 5px;\n//}\n</